CREATE TABLE `materias` (
	`id` INT(11) NOT NULL AUTO_INCREMENT COMMENT 'id da materia',
	`materia` VARCHAR(50) NOT NULL DEFAULT '0' COMMENT 'nome da materia' COLLATE 'utf8mb4_general_ci',
	`data_cad` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'data do cadastro',
	`data_exc` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'data da exclusao',
	PRIMARY KEY (`id`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
;

